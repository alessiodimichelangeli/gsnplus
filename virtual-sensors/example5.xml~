<virtual-sensor name="example5" priority="11">
	<processing-class>
		<class-name>gsn.vsensor.WebInputVirtualSensor</class-name>
		
		<init-params />
		
		<web-input password="interval">
			<command name="change_interval_value">
				<field name="interval" type="int">Insert the new sampling interval (msec).</field>
			</command>
			
			<command name="select_web_command">
				<field name="command 1|2|3" type="int">RED LED:insert 1 to turn on, insert 2 to turn off, insert 3 to let it blink. </field>
			</command>
			
			
	        </web-input>
		
		<output-structure>
			<field name="version" type="int" />
			<field name="id" type="int"/>
			<field name="count" type="int"/>
			<field name="interval" type="int" />
			<field name="readingsTemp0" type="int" />
			<field name="readingsTemp1" type="int" />
			<field name="readingsHum0" type="int" />
			<field name="readingsHum1" type="int" />
			<field name="readingsLum0" type="int" />
			<field name="readingsLum1" type="int" />
		</output-structure>
		
		
	</processing-class>

	<description></description>
	<life-cycle pool-size="10" />
	<addressing>
		<predicate key="geographical">Not Specified</predicate>
		<predicate key="LATITUDE">37.5419</predicate>
		<predicate key="LONGITUDE">-122.1419</predicate>
	</addressing>
	<storage history-size="10" />
	<streams>
		<stream name="input1">
			<source alias="source1" sampling-rate="1" storage-size="1s">
				<address wrapper="TinyOsWrapper">
					<predicate key="host">localhost</predicate>
					<predicate key="port">9002</predicate>
				</address>
				<query>SELECT * FROM wrapper</query>
			</source>
			<query>SELECT version, id, count, interval, readingsTemp0, readingsTemp1, readingsHum0, readingsHum1, readingLum0, readingLum1 FROM source1</query>
		</stream>
	</streams>
</virtual-sensor>
